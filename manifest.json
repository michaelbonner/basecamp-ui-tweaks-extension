{
  "name": "Basecamp UI Tweaks",
  "description": "Some of Basecamp's UI is a little annoying, this is some of the tweaks I've made.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon/16.png",
    "32": "images/icon/32.png",
    "48": "images/icon/48.png",
    "128": "images/icon/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.basecamp.com/*"],
      "css": ["basecamp-overrides.css"]
    }
  ]
}
